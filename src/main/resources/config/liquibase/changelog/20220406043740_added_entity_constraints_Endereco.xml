<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Endereco.
    -->
    <changeSet id="20220406043740-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="endereco"
                                 constraintName="fk_endereco__usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="professor_id"
                                 baseTableName="endereco"
                                 constraintName="fk_endereco__professor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>
    </changeSet>
</databaseChangeLog>
